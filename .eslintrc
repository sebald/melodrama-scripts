{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8
  },
  "plugins": [
    "react"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "indent": [2, 2],
    "linebreak-style": [2, "unix"],
    "no-console": [0],
    "quotes": [2, "single"],
    "semi": [2, "always"]
  }
}