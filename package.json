{
  "name": "melodrama-scripts",
  "version": "1.0.2",
  "description": "Scripts to create a presentations with Spectacle",
  "bin": {
    "melodrama-scripts": "./bin/melodrama-scripts.js"
  },
  "main": "lib/presentation.js",
  "files": [
    "bin",
    "lib",
    "index.js",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "test": "nyc ava",
    "cover": "./node_modules/.bin/nyc report --reporter=html",
    "dev:init": "node bin/melodrama-scripts init .tmp/example-init",
    "dev:start": "node bin/melodrama-scripts start example/index.js",
    "dev:build": "node bin/melodrama-scripts build example/index.js"
  },
  "engines": {
    "node": ">=6"
  },
  "author": {
    "name": "Sebastian Sebald",
    "email": "sebastian.sebald@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebald/melodrama-scripts.git"
  },
  "bugs": {
    "url": "https://github.com/sebald/melodrama-scripts/issues"
  },
  "homepage": "https://github.com/sebald/melodrama-scripts#readme",
  "ava": {
    "source": [
      "lib/*.js"
    ],
    "babel": {
      "presets": [
        "es2015",
        "react",
        "stage-0"
      ]
    }
  },
  "dependencies": {
    "babel-core": "6.18.2",
    "babel-loader": "6.2.7",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "command-exists": "1.0.2",
    "css-loader": "0.25.0",
    "detect-port": "1.0.5",
    "execa": "0.5.0",
    "file-loader": "0.9.0",
    "fs-extra": "1.0.0",
    "got": "6.6.1",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.1",
    "inquirer": "1.2.2",
    "json-loader": "0.5.4",
    "log-symbols": "1.0.2",
    "markdown-loader": "0.1.7",
    "meow": "3.7.0",
    "open": "0.0.5",
    "ora": "0.3.0",
    "pretty-bytes": "4.0.2",
    "pretty-error": "2.0.2",
    "pretty-ms": "2.1.0",
    "raw-loader": "0.5.1",
    "read-pkg": "2.0.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2",
    "write-pkg": "2.0.0"
  },
  "devDependencies": {
    "ava": "0.16.0",
    "eslint": "3.9.1",
    "eslint-plugin-react": "6.5.0",
    "nyc": "9.0.1",
    "proxyquire": "1.7.10",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "sinon": "1.17.6",
    "spectacle": "1.2.5",
    "uuid": "2.0.3"
  }
}
